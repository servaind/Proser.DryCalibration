<Window x:Class="Proser.DryCalibration.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proser.DryCalibration.App"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"     
        xmlns:s="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="Dry Calibration" Height="660" Width="1080"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Regular"
        TextElement.FontSize="13"
        TextOptions.TextFormattingMode="Ideal"
        TextOptions.TextRenderingMode="Auto"
        Background="{DynamicResource MaterialDesignPaper}"
        FontFamily="{DynamicResource MaterialDesignFont}" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="None" Foreground="White" x:Name="MainWindows">

    <Window.Resources>
        <Storyboard x:Key="OpenMenu">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMenu">
                <EasingDoubleKeyFrame KeyTime="0" Value="50"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="200"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMain">
                <EasingDoubleKeyFrame KeyTime="0" Value="1030"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="880"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="CloseMenu">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMenu">
                <EasingDoubleKeyFrame KeyTime="0" Value="200"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="50"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMain">
                <EasingDoubleKeyFrame KeyTime="0" Value="880"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="1030"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>

    <Window.Triggers>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="ButtonOpenMenu">
            <BeginStoryboard Storyboard="{StaticResource OpenMenu}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="ButtonCloseMenu">
            <BeginStoryboard Storyboard="{StaticResource CloseMenu}"/>
        </EventTrigger>
    </Window.Triggers>

    <Grid Background="WhiteSmoke" MouseDown="Window_MouseDown">
        <Grid  Height="70" VerticalAlignment="Top" Background="#FFF79D31"  >
            <TextBlock Text="Ensayo - Calibración en Seco" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" FontFamily="Lusida Sans"/>
            <StackPanel VerticalAlignment="Center" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0 0 20 0" >
                <materialDesign:PackIcon Kind="User" VerticalAlignment="Center" Width="30" Height="30" Margin="5"/>
                <materialDesign:PopupBox PlacementMode="BottomAndAlignRightEdges" StaysOpen="False" FontFamily="Lusida Sans">
                    <StackPanel Width="240">
                        <Button x:Name="btnUserConfig" Click="ButtonPopUpUserConfig_Click" Height="45">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <materialDesign:PackIcon Kind="Settings" Width="25" Height="25" Margin="0 10 10 0"/>
                                <TextBlock Text="Responsable del ensayo " VerticalAlignment="Center" Margin="0 10 0 0"/>
                            </StackPanel>
                        </Button>
                        <Separator/>
                        <Button x:Name="ButtomPopUpLogOut" Content="Salir" Click="ButtomPopUpSalir_Click" Height="45"/>
                    </StackPanel>
                </materialDesign:PopupBox>
            </StackPanel>
        </Grid>

        <!-- Menu Principal -->
        <Grid x:Name="GridMenu" Width="200" HorizontalAlignment="Left" Background="#FFC85019">
            <StackPanel>
                <Grid Height="150" Background="White" >
                    <Button x:Name="ButtonCloseMenu" KeyboardNavigation.IsTabStop="false" Click="ButtonCloseMenu_Click"  Width="50" Height="30" Background="{x:Null}" BorderBrush="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Right">
                        <materialDesign:PackIcon Foreground="#FFC85019" Kind="ArrowLeft" Width="27" Height="27"/>
                    </Button>
                    <Button x:Name="ButtonOpenMenu" Click="ButtonOpenMenu_Click" Width="50" Height="30" Background="{x:Null}" BorderBrush="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="2" Visibility="Collapsed">
                        <materialDesign:PackIcon Foreground="#FFC85019" Kind="Menu" Width="27" Height="27"/>
                    </Button>
                    <StackPanel  Background="#FFF79D31" Height="110" VerticalAlignment="Bottom" >
                        <Image x:Name="ImageLogo" Margin="30" Source="pack://application:,,,/Resources/CAISER_logo_short_min.png" Stretch="None"/>
                    </StackPanel>

                </Grid>

                <ListView Foreground="#FFF79D31" ScrollViewer.HorizontalScrollBarVisibility="Disabled" FontSize="16">

                    <ListViewItem Uid="0" x:Name ="mnuConfiguration" Height="60" Foreground="WhiteSmoke" Background="#FFC32D2F" MouseUp="MnuMain_MouseUp">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon  Kind="Settings" Height="27" Width="27" VerticalAlignment="Center" Margin="3"/>
                            <TextBlock Text="Configuración" VerticalAlignment="Center" Margin="15 10"/>
                        </StackPanel>
                    </ListViewItem>

                    <ListViewItem Uid="1" x:Name="mnuInit"  Height="60" Foreground="WhiteSmoke" Background="#FFC32D2F" MouseUp="MnuMain_MouseUp">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon  Kind="Stopwatch" Height="27" Width="27" VerticalAlignment="Center" Margin="3"/>
                            <TextBlock Text="Iniciar ensayo" VerticalAlignment="Center" Margin="15 10"/>
                        </StackPanel>
                    </ListViewItem>

                    <ListViewItem Uid="2" x:Name="mnuCancel" Height="60" Foreground="WhiteSmoke" Background="#FFC32D2F"  MouseUp="MnuMain_MouseUp">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon  Kind="StopwatchOff" Height="27" Width="27" VerticalAlignment="Center" Margin="3"/>
                            <TextBlock Text="Cancelar ensayo" VerticalAlignment="Center" Margin="15 10"/>
                        </StackPanel>
                    </ListViewItem>

                    <ListViewItem Uid="3" x:Name="mnuReport" Height="60" Foreground="WhiteSmoke" Background="#FFC32D2F"  MouseUp="MnuMain_MouseUp">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon  Kind="FileReport" Height="27" Width="27" VerticalAlignment="Center" Margin="3"/>
                            <TextBlock Text="Ver reporte" VerticalAlignment="Center" Margin="15 10"/>
                        </StackPanel>
                    </ListViewItem>
                </ListView>
            </StackPanel>
        </Grid>

        <Grid x:Name="GridMain" Margin="0 0 0 0"  Width="880" Height="600" HorizontalAlignment="Right" VerticalAlignment="Bottom" Background="WhiteSmoke">
            <!--Usuario-->
            <Grid x:Name="GridConfigurationUser" Visibility="Visible">
                <Grid Width="300" Height="160" Background="White" materialDesign:ShadowAssist.Darken="True" >
                    <StackPanel VerticalAlignment="Center">
                        <Label Content="Ingrese su nombre:" Height="25" FontFamily="Lusida Sans" FontSize="12"  Margin="5 5 0 0" Foreground="DarkGray"/>
                        <Separator Margin="10 0 10 5" />
                        <StackPanel  Orientation="Horizontal" Margin="5 5 0 0">
                            <materialDesign:PackIcon Kind="User" Foreground="Orange" Width="25" Height="25" Margin="0 10 0 0 "/>
                            <TextBox x:Name="txtUserName" Style="{ StaticResource MaterialDesignFloatingHintTextBox }" Margin="10 0 0 0" FontSize="14" Foreground="DarkGray"  Height="40" Width="250" Background="White" HorizontalAlignment="Left"/>
                        </StackPanel>
                        <Button x:Name ="btnUserAccept"  Content="Aceptar" FontSize="12" Width="80" Height="30" Margin="0 15 0 0" FontFamily="Lucida Sans" Click="BtnUserAccept_Click"/>
                    </StackPanel>
                </Grid>
            </Grid>
            <!-- Configuración -->
            <Grid x:Name="GrigConfigurationDevice" Visibility="Hidden">
                <StackPanel Background="WhiteSmoke">
                    <Grid Height="38" >
                        <StackPanel>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Margin="5 0 0 0">
                                <Button Uid="0" Width="200" Content="Configuración General" Foreground="DarkOrange" Background="{x:Null}" BorderBrush="{x:Null}" Click="ConfigItemTabButton_Click" />
                                <Button Uid="1" Width="200" Content="Ajuste NI cDAQ-9184" Foreground="DarkOrange" Background="{x:Null}" BorderBrush="{x:Null}" Click="ConfigItemTabButton_Click" />
                                <Button Uid="2" Width="200" Content="Comunicación Ultrasónico" Foreground="DarkOrange" Background="{x:Null}" BorderBrush="{x:Null}" Click="ConfigItemTabButton_Click"/>
                                <Button Uid="3" Width="200" Content="Puesta en Marcha" Foreground="DarkOrange" Background="{x:Null}" BorderBrush="{x:Null}" Click="ConfigItemTabButton_Click" />
                            </StackPanel>
                            <Grid x:Name="GridCursor" Width="190" Height="5" Background="OrangeRed" HorizontalAlignment="Left" Margin="10 0 0 0"/>
                        </StackPanel>
                    </Grid>
                    <Grid x:Name="ConfigGridTabBody" Height="480" Background="WhiteSmoke" >
                        <!-- Cofiguración General -->
                        <Grid x:Name="ConfigGeneral" HorizontalAlignment="Left" Visibility="Visible">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="570" />
                                <ColumnDefinition Width="310" />
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Column="0">
                                <StackPanel Margin="10 40 0 0" VerticalAlignment="Top">
                                    <Label Content="Medidor Ultrasónico:"  Foreground="DarkOrange"/>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Marca | Fabricante:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Marca" KeyboardNavigation.TabIndex="0" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasBrand" Margin="70,0,0,3"  Width="162" TextAlignment="Left" FontSize="12" Foreground="DarkGray"  Background="White"/>
                                        <Label Content="|"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Fabricante" KeyboardNavigation.TabIndex="0" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasMaker" Margin="0,0,0,3"  Width="163" TextAlignment="Left" FontSize="12" Foreground="DarkGray"  Background="White"/>

                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Modelo | DN | Sch | Serie:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Modelo" KeyboardNavigation.TabIndex="1" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasModel" Margin="39 0 0 0" Width="75"  TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                        <Label Content="|"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="DN" KeyboardNavigation.TabIndex="1" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasDN" Margin="0 0 0 0" Width="75"  TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                        <Label Content="|"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Sch" KeyboardNavigation.TabIndex="1" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasSch" Margin="0 0 0 0" Width="75"  TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                        <Label Content="|"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Serie" KeyboardNavigation.TabIndex="1" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasSerie" Margin="0 0 0 0" Width="74"  TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>

                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Número de serie | identificación:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox Margin="1 0 0 0" ToolTip="Número de serie" KeyboardNavigation.TabIndex="2" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasSerieNumber" Width="162" TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                        <Label Content="|"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox Margin="1 0 0 0" ToolTip="Identificación" KeyboardNavigation.TabIndex="2" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenUltrasIdentification" Width="163" TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Versión de firmware:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Versión de firmware" KeyboardNavigation.TabIndex="3" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" Margin="63 0 0 0" x:Name="confGenUltrasFirmwareVersion" Width="339" TextAlignment="Left"  FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Determinaciones requeridas:"  Foreground="Gray"  FontSize="12"/>
                                        <Label  Margin="16 1 0 0" x:Name="confGenRequiredDetermination" Width="339" Content="Ensayo de verificación de flujo cero y velocidad de sonido." FontSize="12" Foreground="DarkGray" VerticalAlignment="Top"/>
                                    </StackPanel>

                                </StackPanel>

                                <StackPanel Margin="10 220 0 0" VerticalAlignment="Top">
                                    <Label Content="Solicitante:"  Foreground="DarkOrange"/>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Razón social:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Razón social" KeyboardNavigation.TabIndex="4" PreviewMouseDown="ConfigurationGeneralItem_MouseDown"  x:Name="confGenPetitionerBusinessName" Width="339" Margin="101 0 0 0" TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <TextBox ToolTip="Dirección | CP | Localidad | Provincia | País" KeyboardNavigation.TabIndex="5" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenPetitionerBusinessAddress" Margin="182 0 0 0" Width="339" TextAlignment="Left" FontSize="12" Foreground="DarkGray"  Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Lugar de realización:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Lugar físico o nombre de la planta o nombre de la locación" KeyboardNavigation.TabIndex="6" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenPetitionerRealizationPlace" Width="339" Margin="62 0 0 0" TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <TextBox ToolTip="Dirección | CP | Localidad | Provincia | País" KeyboardNavigation.TabIndex="7" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenPetitionerRealizationAddress" Margin="182 0 0 0" Width="339"  TextAlignment="Left" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1">
                                <StackPanel Margin="10 40 0 0" VerticalAlignment="Top">
                                    <Label Content="Ensayo:"  Foreground="DarkOrange"/>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Informe Nº:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox Margin="10 0 0 0" ToolTip="DC-YY-NNNN-MM" KeyboardNavigation.TabIndex="8" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenCalibrationInformationReportNumber"  Width="180" TextAlignment="Left" FontSize="12" Foreground="DarkGray"  Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Responsable:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Responsable del ensayo" KeyboardNavigation.IsTabStop="False" IsReadOnly="True" x:Name="confGenCalibrationInformationResponsible"  Width="180" TextAlignment="Left" Text="Usuario ingresado" FontSize="12" Foreground="DarkGray"  Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Fecha:"  Foreground="Gray"  FontSize="12"/>
                                        <TextBox ToolTip="Fecha de realización" KeyboardNavigation.IsTabStop="False"  x:Name="confGenCalibrationInformationCalibrationDate" IsReadOnly="True" Margin="36 0 0 0" Width="180"  TextAlignment="Left" Text="Fecha de la PC" FontSize="12" Foreground="DarkGray" Background="White" VerticalAlignment="Top"/>
                                    </StackPanel>
                                </StackPanel>

                                <StackPanel Margin="10 220 0 0" VerticalAlignment="Top">
                                    <Label Content="Condiciones ambientales:"  Foreground="DarkOrange"/>
                                    <StackPanel Margin="20 0 0 0"  Orientation="Horizontal" Height="30">
                                        <Label Content="Presión atmosférica (hPa):"  Foreground="Gray"  FontSize="12" VerticalContentAlignment="Center"/>
                                        <xctk:MaskedTextBox ToolTip="Ingrese un valor..." KeyboardNavigation.TabIndex="9" PreviewMouseDown="ConfigurationGeneralItem_MouseDown" x:Name="confGenEnvironmentalConditionAtmosphericPressure" VerticalContentAlignment="Center" Mask="0000"
                                                            ValueDataType="{x:Type s:String}" FontSize="12" Height="26" Width="50" Margin="5 0 0 0" 
                                                            Foreground="Gray" HorizontalAlignment="Left" Background="White" />

                                    </StackPanel>

                                </StackPanel>
                            </Grid>
                            
                            <StackPanel Grid.Column="1" VerticalAlignment="Bottom" Margin="0 0 0 20">
                                <Button x:Name="btnSaveGeneralConfiguration" KeyboardNavigation.TabIndex="10" Click="BtnSaveGeneralConfiguration_Click"  Content="Guardar" BorderBrush="ForestGreen" Background="ForestGreen" Height="27" Width="80" FontSize="12" Margin="40 10 0 0"  HorizontalAlignment="Left"/>
                            </StackPanel>
                          
                        </Grid>
                        <!-- Ajuste NI cDAQ-9184 -->
                        <Grid x:Name="DAQAdjustment" HorizontalAlignment="Left" Visibility="Hidden">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="620" />
                                <ColumnDefinition Width="255" />
                            </Grid.ColumnDefinitions>

                            <StackPanel Margin="15" Grid.Column="0">
                                <Label Content="Sensores de Temperatura:"  Foreground="DarkOrange"/>

                                <StackPanel Orientation="Horizontal">
                                    <StackPanel>
                                        <Label Content="Referencia:"  Foreground="DimGray" Margin="20 0 0 0" Width="60" HorizontalAlignment="Left" FontSize="10"/>
                                        <Separator Width="50" Margin="25 0 0 0" HorizontalAlignment="Left" Background="DimGray"/>
                                        <xctk:MaskedTextBox DataContextChanged="AdjModRefTemp_DataContextChanged"
                                                            x:Name="adjModRefTemp" VerticalContentAlignment="Center" Mask="#00.00" 
                                                            ValueDataType="{x:Type s:String}" FontSize="12" Height="26" Width="50" 
                                                            Foreground="DimGray"  Margin="25 0 0 0" HorizontalAlignment="Left" Background="White" />
                                    </StackPanel>
                                    <Button  x:Name="btnAdjPoint1" Click="BtnAdjPoint1_Click" Margin="15 0 0 0"  Height="46" Width="80" FontSize="12" VerticalAlignment="Bottom" >
                                        <TextBlock>Ajustar<LineBreak/><TextBlock HorizontalAlignment="Center" x:Name="adjLblBtnPoint1" Margin="0 5 0 0" Text="Punto 1"/></TextBlock>
                                    </Button>
                                    <Button  x:Name="btnAdjPoint2" Click="BtnAdjPoint2_Click" Margin="10 0 0 0"  Height="46" Width="80" FontSize="12" VerticalAlignment="Bottom" >
                                        <TextBlock>Ajustar<LineBreak/><TextBlock HorizontalAlignment="Center" x:Name="adjLblBtnPoint2" Margin="0 5 0 0" Text="Punto 2"/></TextBlock>
                                    </Button>
                                    <Button  x:Name="btnAdjPoint3" Click="BtnAdjPoint3_Click" Margin="10 0 0 0"  Height="46" Width="80" FontSize="12" VerticalAlignment="Bottom" >
                                        <TextBlock>Ajustar<LineBreak/><TextBlock HorizontalAlignment="Center" x:Name="adjLblBtnPoint3" Margin="0 5 0 0" Text="Punto 3"/></TextBlock>
                                    </Button>
                                    <Button  x:Name="btnAdjPoint4" Click="BtnAdjPoint4_Click" Margin="10 0 0 0"  Height="46" Width="80" FontSize="12" VerticalAlignment="Bottom" >
                                        <TextBlock>Ajustar<LineBreak/><TextBlock HorizontalAlignment="Center" x:Name="adjLblBtnPoint4" Margin="0 5 0 0" Text="Punto 4"/></TextBlock>
                                    </Button>
                                    <Button  x:Name="btnAdjPoint5" Click="BtnAdjPoint5_Click" Margin="10 0 0 0"  Height="46" Width="80" FontSize="12" VerticalAlignment="Bottom" >
                                        <TextBlock>Ajustar<LineBreak/><TextBlock HorizontalAlignment="Center" x:Name="adjLblBtnPoint5" Margin="0 5 0 0" Text="Punto 5"/></TextBlock>
                                    </Button>
                                </StackPanel>
                                <StackPanel Margin="25 0 0 0">
                                    <StackPanel  Margin="0 5 0 0"  Height="50" Orientation="Horizontal"  Background="WhiteSmoke" HorizontalAlignment="Left">
                                        <StackPanel Width="70" HorizontalAlignment="Left">
                                            <Label Content="RTD:" Foreground="DimGray" FontSize="10"/>
                                            <Separator Width="30" HorizontalAlignment="Left" Margin="5 0 0 0" Background="DimGray" />
                                            <Label Content="1" Foreground="DimGray"/>
                                        </StackPanel>
                                        <StackPanel Width="60" HorizontalAlignment="Left">
                                            <Label Content="(ohm)" Foreground="DimGray" FontSize="10" FontWeight="DemiBold"/>
                                            <Separator Width="65" HorizontalAlignment="Left" Background="DimGray" />
                                            <TextBox x:Name="adjResPoint1RTD1" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>

                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <Label Content="(ohm)" Foreground="DimGray" FontSize="10" FontWeight="DemiBold"/>
                                            <Separator Width="65" HorizontalAlignment="Left" Background="DimGray" />
                                            <TextBox x:Name="adjResPoint2RTD1"  IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>

                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <Label Content="(ohm)" Foreground="DimGray" FontSize="10" FontWeight="DemiBold"/>
                                            <Separator Width="65" HorizontalAlignment="Left" Background="DimGray" />
                                            <TextBox x:Name="adjResPoint3RTD1" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>

                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <Label Content="(ohm)" Foreground="DimGray" FontSize="10" FontWeight="DemiBold"/>
                                            <Separator Width="65" HorizontalAlignment="Left" Background="DimGray" />
                                            <TextBox x:Name="adjResPoint4RTD1" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>

                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <Label Content="(ohm)" Foreground="DimGray" FontSize="10" FontWeight="DemiBold"/>
                                            <Separator Width="65" HorizontalAlignment="Left" Background="DimGray" />
                                            <TextBox x:Name="adjResPoint5RTD1" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>

                                        <StackPanel Margin="20 2 0 0" Width="50" HorizontalAlignment="Left">
                                            <Label Content="Incluir:" Foreground="DimGray" FontSize="10"/>
                                            <Separator Width="40" HorizontalAlignment="Left" Margin="5 0 0 0" Background="DimGray" />
                                            <CheckBox x:Name="adjModRTDActChk1"  IsChecked="True"  Margin="10 5 0 0 " Cursor="Hand"/>
                                        </StackPanel>
                                    </StackPanel>
                                    <StackPanel  Margin="0 5 0 0"  Height="24" Orientation="Horizontal"  Background="WhiteSmoke" HorizontalAlignment="Left">
                                        <StackPanel Width="70" HorizontalAlignment="Left">
                                            <Label Content="2" Foreground="DimGray"/>
                                        </StackPanel>
                                        <StackPanel Width="60" HorizontalAlignment="Left">
                                            <TextBox x:Name="adjResPoint1RTD2" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>
                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <TextBox x:Name="adjResPoint2RTD2" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>
                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <TextBox x:Name="adjResPoint3RTD2" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>
                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <TextBox x:Name="adjResPoint4RTD2" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>
                                        <StackPanel Width="60" HorizontalAlignment="Left" Margin="30 0 0 0">
                                            <TextBox x:Name="adjResPoint5RTD2" IsReadOnly="True"  Height="25" Width="65" FontSize="12" Foreground="DarkGray" Background="White" HorizontalAlignment="Left"/>
                                        </StackPanel>
                                        <StackPanel Margin="20 2 0 0" Width="50" HorizontalAlignment="Left">
                                            <CheckBox x:Name="adjModRTDActChk2" IsChecked="True" Margin="10 5 0 0 " Cursor="Hand"/>
